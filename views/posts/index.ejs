<h1>My posts</h1>
<% posts.forEach(function(posts) { %>
    <div>
        <h3><%= posts.title %></h3>
        <div><%= posts.description %></div>
        <div><%= posts.price %></div>
        <div><%= posts.location %></div>
        <div><a href="/posts/<%=posts.id%>">View more</a></div>
    </div>
<% }) %>